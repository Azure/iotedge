          "loadGen2ToRelayer2": "FROM /messages/modules/loadGen2/outputs/output2 INTO BrokeredEndpoint(\"/modules/relayer2/inputs/input2\")",
          "relayer2ToCloud": "FROM /messages/modules/relayer2/outputs/output2 INTO $upstream",
