# Auto detect text files and perform LF normalization
*text=auto

# Declare files that will always have LF line endings on checkout
*.sh text eol=lf
*.rs text eol=lf

# Binary files to ignore and not modify
*.pfx binary
*.ico binary
*.svg binary
*.jpg binary
*.png binary
*.exe binary
*.dll binary
*.pdb binary
*.eot binary
*.woff binary
*.woff2 binary
*.ttf binary
*.otf binary
